mixin filter(mapFilter)
	+b.filter-block&attributes(attributes)
		+e.head.h5.fw-600.mb-4 Фильтр
		+e.close.filter-toggle-js.d-lg-none
		+e.inner
			mixin item(t, ...items)
				+e.group&attributes(attributes)

					+e.title= t
					+e.group-inner
						each item in items
							+in-c("checkbox", item)
						block
			if mapFilter
				+item('Назначение').mapFilter
					+in-c("checkbox",'Многоэтажное жилищное строительство')(checked class="map-dot-input" style="--color-map:#FF0000")
					+in-c("checkbox",'Индивидуальное жилищное строительство')(checked class="map-dot-input" style="--color-map:#00ff00")
					+in-c("checkbox",'Промышленное назначение')(checked class="map-dot-input" style="--color-map:#0000FF")
					+in-c("checkbox",'Коммерческое назначение')(checked class="map-dot-input" style="--color-map:#F8B708")
					+in-c("checkbox",'Рекреационное назначение')(checked class="map-dot-input" style="--color-map:#15E3F0")
					+in-c("checkbox",'Сельскохозяйственное использование')(checked class="map-dot-input" style="--color-map:#FFAAAA")
					+in-c("checkbox",'Многоэтажное жилищное строительство')(checked class="map-dot-input" style="--color-map:#FF0000")
					+in-c("checkbox",'Индивидуальное жилищное строительство')(checked class="map-dot-input" style="--color-map:#00ff00")
					+in-c("checkbox",'Промышленное назначение')(checked class="map-dot-input" style="--color-map:#0000FF")
					+in-c("checkbox",'Коммерческое назначение')(checked class="map-dot-input" style="--color-map:#F8B708")
					+in-c("checkbox",'Рекреационное назначение')(checked class="map-dot-input" style="--color-map:#15E3F0")
					+in-c("checkbox",'Сельскохозяйственное использование')(checked class="map-dot-input" style="--color-map:#FFAAAA")
			+item(`Регион`,`Москва`,`Московская область`,`Регионы`)
			+item(`Шоссе`,`Алтуфьевское`,`Боровское`,`Варшавское`,`Вашутинское`,`Волковское`,`Волоколамское`,`Варшавское`,`Вашутинское`,`Волковское`,`Волоколамское`)
